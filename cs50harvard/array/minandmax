a=[]
for i in range(4):
    n=int(input("enter the element"))
    a.append(n)
a=list(a)    
for i in range(0,len(a)-1,1):
    for j in range(0,len(a)-1-i,1):
        if a[j]>a[j+1]:
            temp=a[j]
            a[j]=a[j+1]
            a[j+1]=temp
min=a[0]
print(min)       
max=a[len(a)-1] 
print(max)     
      